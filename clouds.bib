@article{Cosofret2009,
abstract = {A sensor constellation capable of determining the location and detailed concentration distribution of chemical warfare agent simulant clouds has been developed and demonstrated on government test ranges. The constellation is based on the use of standoff passive multispectral infrared imaging sensors to make column density measurements through the chemical cloud from two or more locations around its periphery. A computed tomography inversion method is employed to produce a 3D concentration profile of the cloud from the 2D line density measurements. We discuss the theoretical basis of the approach and present results of recent field experiments where controlled releases of chemical warfare agent simulants were simultaneously viewed by three chemical imaging sensors. Systematic investigations of the algorithm using synthetic data indicate that for complex functions, 3D reconstruction errors are less than 20\% even in the case of a limited three-sensor measurement network. Field data results demonstrate the capability of the constellation to determine 3D concentration profiles that account for \~{}?86\%? of the total known mass of material released.},
author = {Cosofret, Bogdan R and Konno, Daisei and Faghfouri, Aram and Kindle, Harry S and Gittins, Christopher M and Finson, Michael L and Janov, Tracy E and Levreault, Mark J and Miyashiro, Rex K and Marinelli, William J},
file = {:C$\backslash$:/Users/amitibo/Documents/Mendeley Desktop/Cosofret et al. - 2009 - Imaging sensor constellation for tomographic chemical cloud mapping.pdf:pdf},
issn = {1539-4522},
journal = {Applied optics},
month = apr,
number = {10},
pages = {1837--52},
pmid = {19340137},
title = {{Imaging sensor constellation for tomographic chemical cloud mapping}},
