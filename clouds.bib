@article{Aides:13,
abstract = {Aerosols affect climate, health and aviation. Currently, their retrieval assumes a plane-parallel atmosphere and solely vertical radiative transfer. We propose a principle to estimate the aerosol distribution as it really is: a three dimensional (3D) volume. The principle is a type of tomography. The process involves wide angle integral imaging of the sky on a very large scale. The imaging can use an array of cameras in visible light. We formulate an image formation model based on 3D radiative transfer. Model inversion is done using optimization methods, exploiting a closed-form gradient which we derive for the model-fit cost function. The tomography model is distinct, as the radiation source is unidirectional and uncontrolled, while off-axis scattering dominates the images.},
author = {Aides, Amit and Schechner, Yoav YY and Holodovsky, Vadim and Garay, Michael J and Davis, Anthony B},
doi = {10.1364/OE.21.025820},
file = {:C$\backslash$:/Users/amitibo/Documents/Mendeley Desktop/Aides et al. - 2013 - Multi sky-view 3D aerosol distribution recovery.pdf:pdf},
journal = {Opt. Express},
keywords = {Aerosol detection,Atmospheric scattering,Inverse problems,Inverse scattering,Passive remote sensing},
number = {22},
pages = {25820--25833},
publisher = {OSA},
title = {{Multi sky-view 3D aerosol distribution recovery}},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-21-22-25820 http://www.opticsinfobase.org/abstract.cfm?uri=oe-21-22-25820},
